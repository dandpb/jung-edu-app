# Supabase Configuration
REACT_APP_SUPABASE_URL=https://YOUR_PROJECT.supabase.co
REACT_APP_SUPABASE_ANON_KEY=YOUR_ANON_KEY
REACT_APP_SUPABASE_SERVICE_ROLE_KEY=YOUR_SERVICE_ROLE_KEY

# Admin Configuration (Generate secure values)
REACT_APP_ADMIN_EMAIL=admin@example.com
# Generate password hash using: npx bcrypt-cli hash "your-secure-password" 12
REACT_APP_ADMIN_PASSWORD_HASH=
REACT_APP_ADMIN_SALT=

# OpenAI Configuration (Optional)
REACT_APP_OPENAI_API_KEY=

# Application Settings
REACT_APP_API_BASE_URL=https://api.your-domain.com
REACT_APP_APP_NAME=JaqEdu
REACT_APP_APP_VERSION=1.0.0

# Security Settings
REACT_APP_JWT_SECRET=generate-a-secure-64-char-secret
REACT_APP_SESSION_TIMEOUT=3600000
REACT_APP_MAX_LOGIN_ATTEMPTS=5
REACT_APP_LOCKOUT_DURATION=1800000

# Feature Flags
REACT_APP_ENABLE_AI_FEATURES=false
REACT_APP_ENABLE_ANALYTICS=true